/* To learn more about this file see: https://angular.io/config/tsconfig. */
{
  "extends": "./tsconfig.json",
  "compilerOptions": {
    "baseUrl": ".",
    "outDir": "./out-tsc/app",
    "types": [
      "node"
    ],
    "paths": {
      "@pages/*": ["src/@pages/*"],
      "@components/*": ["src/@components/*"],
      "@layout/*": ["src/@layout*"],
      "@directives/*": ["src/@directives/*"],
      "@services/*": ["sourceMap/@services/*"],
    },
  },
  "files": [
    "src/main.ts",
    "src/main.server.ts",
    "server.ts"
  ],
  "include": [
    "src/**/*.d.ts"
  ],

}
